<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.test.model.Templatecollectionentity" table="templatecollectionentity" schema="denim_store_db">
        <composite-id mapped="true" class="com.test.model.TemplatecollectionentityPK">
            <key-property name="templateId">
                <column name="TemplateId" sql-type="char(10)" length="10"/>
            </key-property>
            <key-property name="userId">
                <column name="UserId" sql-type="char(10)" length="10"/>
            </key-property>
        </composite-id>
    </class>
</hibernate-mapping>